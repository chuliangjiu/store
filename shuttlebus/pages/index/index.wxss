/**index.wxss**/
.container{
  width: 750rpx;
  padding: 12rpx;
  box-sizing: border-box;
  background-color: #002e69;
  display: flex;
  flex-direction: column;
  justify-content:space-between;
  align-items: center;
}
.input-wrapper{
  width: 100%;
  height: 24%;
}
.map-wrapper{
  width: 100%;
  height: 74%;
}
.map-center-img{
  width: 60rpx;
  height: 60rpx;
  box-sizing: border-box;
  position: absolute;
  top: calc(50% - 30rpx);
  left: calc(50% - 30rpx);
}
.logout-img{
  width: 80rpx;
  height: 80rpx;
  box-sizing: border-box;
  position: absolute;
  bottom: calc(10% - 40rpx);
  right: calc(10% - 40rpx);
}